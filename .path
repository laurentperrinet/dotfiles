# PATH additions
# export PATH="~/bin:$PATH"
export PATH=$HOME/bin:$PATH
export PATH=$HOME/Library/Python/2.7/bin:$PATH
export PYTHONPATH=$HOME/Library/Python/2.7/lib/python/site-packages
# NEST
export PATH=/opt/nest/bin:$PATH
export PYTHONPATH=/opt/nest/lib/python2.7/site-packages
# MacTex
export PATH=/usr/texbin:$PATH
# Inkscape.app
export PATH=$PATH:/Applications/Inkscape.app/Contents/Resources/bin
# HomeBrew
export PATH=/usr/local/bin:/usr/local/sbin:$HOME/.gem/ruby/1.8/bin:$PATH
# HomeBrew/python
export PYTHONPATH=/usr/local/lib/python2.7/site-packages:/usr/local/opt/vtk5/lib/python2.7/site-packages:$PYTHONPATH
export PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
export MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"
